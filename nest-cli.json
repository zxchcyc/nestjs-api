{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": ["@nestjs/swagger/plugin"],
    "assets": ["**/i18n/**/*"]
  },
  "plugins": [
    {
      "name": "@nestjs/swagger/plugin",
      "options": {
        "classValidatorShim": true
      }
    }
  ]
}
